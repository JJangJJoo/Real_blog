<html>
  <head>
    <title>Real blog</title>
  </head>
  <body>
    <h1>Welcome To Real Blog!</h1>
    <% @posts.each do |post| %>
      <p>제목: <%= post.title %></p>
      <p>내용: <%= post.body %></p>
    <% end %>
    <form action = "/create">
      제목 : <input type = "text" name = "title">
      내용 : <input type = "text" name = "content">
      <input type = "submit">
    </form>
  </body>
</html>
